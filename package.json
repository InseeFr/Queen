  "private": true,
  "dependencies": {
    "@inseefr/lunatic": "^2.0.0-rc5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "axios": "^0.19.2",
    "dexie": "^2.0.4",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-keyboard-event-handler": "^1.5.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-shadow": "^18.1.0",
    "styled-components": "^5.0.1"
  },
  "scripts": {
    "start": "set PORT=5000 && react-scripts start",
    "build": "env PUBLIC_URL=__PUBLIC_URL_TO_REPLACE__ react-scripts build && node ./configuration/build/build-sw.js && yarn post-build",
    "post-build": "node ./configuration/build/manage-public-url.js && copy-and-watch configuration/configuration.json build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false --coverage --testResultsProcessor ./node_modules/jest-sonar-reporter",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "copy-and-watch": "^0.1.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^1.18.2"
  }
}
